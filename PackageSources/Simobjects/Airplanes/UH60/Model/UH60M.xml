  <PartInfo>
    <Name>RTR_Rot</Name>
    <AnimLength>100</AnimLength>
    <Animation>
      <Parameter>
        <Code>
         
        (A:ROTOR RPM PCT:1, percent) 40 &gt;
        if{
          (E:ABSOLUTE TIME, second) (L:uh60_rtr_SPD, enum) / 1 % 100 *
        } els{ 
          (A:PROP ROTATION ANGLE:1, radians) 15.92 * 
        }

        </Code>
      </Parameter>
    </Animation>
  </PartInfo>
  
